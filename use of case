select fname,lname,
case 
when salary>=40000 then 'Higher Salary'
else 'Lower Salary'
end
as 'SELECT CATEGORY'
FROM employees;

-- remember to put comma in the end of the select query 

select patient_id,name,age,
case 
when age>=18 and age<=29 then 'Young Adult'
when age>=30 and age<=39 then 'Adult'
when age>=40 then 'Middle Aged'
end as age_group
from patients
order by patient_id asc;
